  "flex-layout.col#buybutton":{
    "children": [
      "add-to-cart-button#vitrine"
    ],
    "props":{
      "width": "100%",
      "verticalAlign": "middle",
      "blockClass": "buyc",
      "paddingLeft": "5px",
      "paddingRight": "0px"
    }
  },
  
  "flex-layout.col#buybuttonNotLog":{
    "children": [
      "rich-text#VERPRODUTO"
    ],
    "props":{
      "width": "100%",
      "verticalAlign": "middle",
      "blockClass": "buycVER",
      "paddingLeft": "5px",
      "paddingRight": "0px"
    }
  },
  "product-quantity#vitrine":{
    "props":{
      "size":"small"
    }
  },
  "add-to-cart-button#vitrine":{
      "props":{
        "text": "Agregar al carrito"
      }
  },
  "add-to-cart-button#vitrineNotLog":{
    "props":{
      "text": "Ver Producto"
    }
},
"rich-text#VERPRODUTO": {
  "props": {
    "text": "VER PRODUCTO",
    "textAlignment": "CENTER",
    "font": "t-heading-3",
    "textPosition": "CENTER",
    "blockClass": "VERPRODUCTOBOTON"
  }
},
  "flex-layout.row#pricing":{
    "children": [
      "product-summary-price#first"
    ]
  },
  "product-summary-price#first":{
    "props":{
      "blockClass": "newprice"
    }
  },

  "challenge.trade-policy-condition": {
    "props": {
      "redirectPath"  : "/login"
    }
  }


